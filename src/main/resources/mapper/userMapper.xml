<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.devcamp.eztour.dao.UserMapper">

    <insert id="insertUser" parameterType="UserDto">
        INSERT INTO
                test
                (usr_id, cmn_cd_prf_img, cmn_cd_usr_stt, pwd, usr_nm)
        VALUES
                (#{usr_id},
                 #{cmn_cd_prf_img},
                 #{cmn_cd_usr_stt},
                 #{pwd},
                 #{usr_nm})
    </insert>

    <insert id="insertUserHistory" parameterType="User">
        INSERT INTO
            test_history
        VALUES
            (#{user_id},
             now(),
             null,
             null)
    </insert>


</mapper>