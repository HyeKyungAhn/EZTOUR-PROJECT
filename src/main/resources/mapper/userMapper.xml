<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.devcamp.eztour.dao.UserMapper">

    <insert id="insertUser" parameterType="UserDto">
        INSERT
                INTO usr
                (usr_id,
                 cmn_cd_usr_stt,
                 pwd,
                 usr_nm,
                 brth,
                 gndr,
                 email,
                 phn,)
        VALUES
                (#{usr_id},
                 '2a',
                 #{pwd},
                 #{usr_nm},
                 #{brth},
                 #{gndr},
                 #{email},
                 #{phn})
    </insert>

    <insert id="insertUserHistory" parameterType="UserDto">
        INSERT INTO
            test_history
            (user_id, user_reg_date)
        VALUES
            (#{usr_id},
             now()
            )
    </insert>


</mapper>